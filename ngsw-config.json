{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ],
        "urls": [
          "https://akademy-72cc4.firebaseapp.com/*.html"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "dataApi",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "urls": [
        "https://akademyapi.herokuapp.com/**",
        "https://akademy-72cc4.firebaseapp.com/alunos/**",
        "https://akademy-72cc4.firebaseapp.com/planos/**",
        "https://akademy-72cc4.firebaseapp.com/turmas/**",
        "https://akademy-72cc4.firebaseapp.com/listturmas/**",
        "https://akademy-72cc4.firebaseapp.com/instrutores/**",
        "https://akademy-72cc4.firebaseapp.com/financeiro/**"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "1d",
        "strategy": "freshness"
      }
    }
  ]
}